package com.hexacta.booster.codegeneration.template;

import java.util.Map;

import com.hexacta.booster.codegeneration.template.model.MetaTemplate;
import com.hexacta.booster.codegeneration.template.model.MetaVariable;

/**
 * @author ltenconi
 * 
 */

public class MetaTemplateGeneration {

    private MetaTemplate metaTemplate;

    private Map<MetaVariable, Object> metaVariablesValue;

    /**
     * @param metaTemplate
     * @param metaVariablesValue
     */
    public MetaTemplateGeneration(final MetaTemplate metaTemplate, final Map<MetaVariable, Object> metaVariablesValue) {
        super();
        this.metaTemplate = metaTemplate;
        this.metaVariablesValue = metaVariablesValue;
    }

    /**
     * @return the metaTemplate
     */
    public MetaTemplate getMetaTemplate() {
        return metaTemplate;
    }

    /**
     * @return the metaVariablesValue
     */
    public Map<MetaVariable, Object> getMetaVariablesValue() {
        return metaVariablesValue;
    }

}
