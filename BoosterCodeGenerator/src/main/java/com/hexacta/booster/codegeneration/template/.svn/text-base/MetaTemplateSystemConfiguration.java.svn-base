package com.hexacta.booster.codegeneration.template;

/**
 * 
 * @author fmrodriguez
 * 
 */
public class MetaTemplateSystemConfiguration {

    private ITemplatePersister templatePersister;

    private IMetaTemplateEngine metaTemplateEngine;

    /**
     * @param metaTemplateEngine
     * @param templatePersister
     */
    public MetaTemplateSystemConfiguration(final IMetaTemplateEngine metaTemplateEngine,
            final ITemplatePersister templatePersister) {
        super();
        this.metaTemplateEngine = metaTemplateEngine;
        this.templatePersister = templatePersister;
    }

    public ITemplatePersister getTemplatePersister() {
        return templatePersister;
    }

    public IMetaTemplateEngine getMetaTemplateEngine() {
        return metaTemplateEngine;
    }

}
